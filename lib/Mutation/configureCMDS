 -DENABLE_TCMALLOC=true \
 -DENABLE_TESTS=true \
 -DENABLE_UNIT_TESTS=OFF \
 -DGTEST_SRC_DIR=../src/unittests/ \
 -DKLEE_UCLIBC_PATH=/media/thierry/TestMutants/KLEE-MART/solvers_uclib/klee-uclibc \
